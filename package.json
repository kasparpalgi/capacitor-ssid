{
    "name": "capacitor-ssid-plugin",
    "version": "1.2.2",
    "description": "Simple SSID detection plugin for Capacitor",
    "main": "dist/plugin.cjs.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "unpkg": "dist/plugin.js",
    "files": [
        "android/",
        "dist/",
        "ios/",
        "CapacitorSsidPlugin.podspec"
    ],
    "author": "Kaspar L. Palgi",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/kasparpalgi/capacitor-ssid.git"
    },
    "bugs": {
        "url": "https://github.com/kasparpalgi/capacitor-ssid/issues"
    },
    "keywords": [
        "capacitor",
        "plugin",
        "wifi",
        "ssid"
    ],
    "scripts": {
        "verify": "npm run verify:web",
        "verify:web": "npm run build",
        "build": "npm run clean && tsc && rollup -c rollup.config.js",
        "clean": "rimraf ./dist",
        "watch": "tsc --watch",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@capacitor/android": "^7.0.0",
        "@capacitor/core": "^7.0.0",
        "@capacitor/ios": "^7.0.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.79.1",
        "typescript": "^4.9.5"
    },
    "peerDependencies": {
        "@capacitor/core": "^7.0.0"
    },
    "capacitor": {
        "ios": {
            "src": "ios"
        },
        "android": {
            "src": "android"
        }
    }
}